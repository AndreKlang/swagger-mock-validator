  },
  "engines": {
    "node": ">=4.4.7"
  },
  "devDependencies": {
    "conventional-changelog": "^1.1.0",
    "conventional-changelog-lint": "^1.1.0",
    "eslint": "3.9.1",
    "express": "^4.14.0",
    "gulp": "^3.9.1",
    "gulp-bump": "^2.5.0",
    "gulp-cli": "^1.2.1",
    "gulp-conventional-changelog": "^1.1.0",
